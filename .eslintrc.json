{
  "extends": "airbnb",
  "plugins": [
    "react",
    "jsx-a11y",
    "import"
  ],
  "parser": "babel-eslint",
  "globals": {
    "document": true,
    "window": true,
    "fetch": true
  },
  "rules": {
    "semi": ["error", "never"],
    "no-multi-spaces": [2, { "exceptions": { "VariableDeclarator": true }}],
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"]}],
    "import/no-extraneous-dependencies": [0],
    "import/no-unresolved": [0],
    "import/extensions": [0]
  }
}
